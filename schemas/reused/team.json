{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "default": "team"
    },
    "players": {
      "type": "array",
      "items": {
        "$ref": "./user.json"
      }
    }
  },
  "required": ["id", "name", "players"]
}
